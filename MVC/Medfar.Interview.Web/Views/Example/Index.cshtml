@model Medfar.Interview.Web.Models.ExampleViewModel
@{
    ViewBag.Title = "Example";
}

@section Scripts
{
    <script src="@Url.Content("~/Scripts/example/example.js")"></script>
}

<h2>@ViewBag.Title.</h2>

<p class="text-success">@ViewBag.StatusMessage</p>
<div>
   <span> Example </span>
</div>


<div class="row">
    <div class="col-lg-12">
        <div class="tb-container">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>First Name</th>
                        <th>Last Name </th>
                        <th>Email</th>
                        <th>Date Created</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var user in Model.Users)
                    {
                        <tr>
                            <td>@user.id</td>
                            <td>@user.first_name</td>
                            <td>@user.last_name</td>
                            <td>@user.email</td>
                            <td>@user.date_created</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
        <button class="btn btn-info" type="button" id="addressSearch" onclick="location.href='@Url.Action("LoadData", "Example")'"> Load Data</button>
    </div>
</div>